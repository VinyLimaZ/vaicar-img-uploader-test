<h1>Upload your image</h1>

<%= render 'partials/object_errors', object: @image %>

<%= form_with model: @image, local: true do |f| %>
  <%= f.label :file %>
  <%= f.file_field :file %>
  <%= f.label :owner %>
  <%= f.text_field :owner %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.submit %>
<% end %>
