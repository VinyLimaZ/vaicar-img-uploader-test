<h1>Images#new</h1>
<p>Find me in app/views/images/new.html.erb</p>
<%= form_with model: @image do |f| %>
  <%= f.label :file %>
  <%= f.file_field :file %>
  <%= f.label :owner %>
  <%= f.text_field :owner %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.submit %>
<% end %>
